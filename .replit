entrypoint = "index.js"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"
packages = ["killall", "sqlite"]

[env]
DATABASE_URL = "file:./dev.db"
OPENAI_API_KEY = "${REPLIT_SECRET_OPENAI_API_KEY}"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "Development Server"

[[workflows.workflow]]
name = "Development Server"
author = 46306395
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run prisma:generate"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Places Proxy Dev"
author = 46306395
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd places-proxy && npm run dev"

[[ports]]
localPort = 24678
externalPort = 80

[[ports]]
localPort = 33013
externalPort = 3003

[[ports]]
localPort = 35775
externalPort = 3002

[[ports]]
localPort = 43327
externalPort = 4200

[[ports]]
localPort = 43719
externalPort = 5173

[[ports]]
localPort = 44939
externalPort = 3001

[[ports]]
localPort = 45161
externalPort = 5000

[[ports]]
localPort = 45571
externalPort = 3000

[agent]
expertMode = true

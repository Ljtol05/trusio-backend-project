{
  "name": "places-proxy",
  "version": "1.0.0",
  "type": "module",
  "description": "Secure proxy service for Google Places API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",

{
  "name": "envelope-app-backend",
  "version": "1.0.0",
  "description": "Multi-agent financial coaching backend with envelope budgeting",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:agents": "vitest run --config vitest.config.agents.ts",
    "test:agents:watch": "vitest --config vitest.config.agents.ts",
    "test:coverage": "vitest run --coverage",
    "test:agents:coverage": "vitest run --config vitest.config.agents.ts --coverage",
    "test:integration": "vitest run src/agents/__tests__/integration.test.ts",
    "test:performance": "vitest run src/agents/__tests__/performance.test.ts",
    "test:validation": "vitest run src/agents/__tests__/validation.test.ts",
    "test:all": "npm run test && npm run test:agents",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "health-check": "tsx -e \"import('./src/agents/health.js').then(m => m.getSystemHealth()).then(console.log)\"",
    "agents:validate": "tsx -e \"import('./src/agents/index.js').then(m => console.log('Validation:', m.validateAgentConfiguration()))\""
  },
  "dependencies": {
    "@openai/agents": "^0.0.17",
    "@prisma/client": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.26.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.0.0",
    "resend": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.55.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "envelope-budgeting",
    "financial-coaching",
    "multi-agent",
    "ai",
    "openai",
    "typescript",
    "express",
    "prisma"
  ],
  "author": "Your Name",
  "license": "MIT"
}
